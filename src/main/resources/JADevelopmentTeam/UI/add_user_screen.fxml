<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="JADevelopmentTeam.UI.AddUserScreenController">
    <children>
        <TextField fx:id="usernameTextField" prefHeight="34.0" prefWidth="559.0" promptText="user name">
            <VBox.margin>
                <Insets left="30.0" right="30.0" top="30.0" />
            </VBox.margin>
        </TextField>
        <PasswordField fx:id="passwordTextField" prefHeight="34.0" prefWidth="559.0" promptText="password">
            <VBox.margin>
                <Insets left="30.0" right="30.0" top="30.0" />
            </VBox.margin>
        </PasswordField>
        <PasswordField fx:id="confirmPasswordTextField" prefHeight="34.0" prefWidth="559.0" promptText="confirm password">
            <VBox.margin>
                <Insets left="30.0" right="30.0" top="30.0" />
            </VBox.margin>
        </PasswordField>
        <JFXComboBox fx:id="userTypeComboBox" prefHeight="56.0" prefWidth="618.0">
            <padding>
                <Insets left="30.0" right="30.0" top="30.0" />
            </padding>
        </JFXComboBox>
        <JFXButton fx:id="addUserButton" buttonType="RAISED" disable="true" prefHeight="60.0" prefWidth="438.0" style="-fx-background-color: white;" text="Add User">
            <font>
                <Font name="MathJax_Caligraphic-Bold" size="31.0" />
            </font>
            <VBox.margin>
                <Insets bottom="30.0" />
            </VBox.margin>
        </JFXButton>
    </children>
</VBox>
